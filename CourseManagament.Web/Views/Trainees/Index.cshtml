@model IEnumerable<CourseManagament.Web.Models.Trainee>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">FullName</th>
            <th scope="col">Email</th>
            <th scope="col">Age</th>
            <th scope="col">Address</th>
            <th scope="col">Education</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.TraineeId</th>
                <td>@item.FullName</td>
                <td>@item.Email</td>
                <td>@item.Age</td>
                <td>@item.Address</td>
                <td>@item.Education</td>
            </tr>
        }
    </tbody>
</table>